<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bank1.loginController">
   <children>
      <TextField fx:id="uname" layoutX="52.0" layoutY="123.0" promptText="Username" />
      <PasswordField fx:id="pswd" layoutX="52.0" layoutY="188.0" promptText="Password" />
      <Button layoutX="104.0" layoutY="261.0" mnemonicParsing="false" onAction="#login" text="LogIn" style="-fx-background-color: #6242ff; -fx-text-fill: white; -fx-background-radius: 10px; -fx-border-radius: 10px;"/>
      <ImageView fitHeight="221.0" fitWidth="341.0" layoutX="235.0" layoutY="78.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/bank.png" />
         </image>
      </ImageView>
      <Hyperlink layoutX="104.0" layoutY="308.0" onAction="#signup" text="SignUp" />
   </children>
</AnchorPane>
